				$("#iva105SNC").val(ventasIva105SNC.toFixed(2));

				$("#totalVentas").val(ventasTotal.toFixed(2));

				console.log(bodyRetenciones);
				$("#bodyRetenciones").html(bodyRetenciones);
				$("#retenciones-total").text(ventasPercepciones.toFixed(2));





    		});

